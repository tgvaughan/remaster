  <depends on="BEAST.app" atleast="2.7.0" atmost="2.7.9"/>

  <service type="beast.base.core.BEASTInterface">
    <provider classname="remaster.Reaction"/>
    <provider classname="remaster.Simulator"/>
    <provider classname="remaster.TypedTreeLogger"/>
    <provider classname="remaster.StochasticTrajectoryStatsLogger"/>
    <provider classname="remaster.PunctualReaction"/>
    <provider classname="remaster.SimulatedTree"/>
    <provider classname="remaster.StochasticTrajectory"/>
    <provider classname="remaster.DeterministicTrajectory"/>
    <provider classname="remaster.CoalescentTrajectory"/>
  </service>
</package>
